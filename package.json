{
  "name": "stry",
  "version": "2.1.0",
  "description": "A mini self-hosted Archive Of Our Own, or a story host with tagging.",
  "scripts": {
    "b:d": "npm-run-all --sequential ts:c ts:b b:m",
    "b:p": "npm-run-all --sequential ts:c ts:b js:c b:m",
    "b:m": "node build.js",
    "js:c": "uglifyjs --compress --source-map \"content='inline'\" --output dist/index.min.js -- dist/index.bundle.js",
    "ts:b": "px --map-inline temp/index.js dist/index.bundle.js",
    "ts:c": "tsc",
    "build": "webpack --mode=production",
    "build:dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/Txuritan/stry2.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/Txuritan/stry2/issues"
  },
  "homepage": "https://gitlab.com/Txuritan/stry2#readme",
  "dependencies": {
    "dateformat": "^3.0.3",
    "marked": "^0.7.0",
    "mithril": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/dateformat": "^3.0.0",
    "@types/marked": "^0.6.5",
    "@types/mithril": "^2.0.0",
    "@types/node": "^12.7.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "git-describe": "^4.0.4",
    "git-revision-webpack-plugin": "^3.0.3",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.22.10",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "uglify-js": "^3.6.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
